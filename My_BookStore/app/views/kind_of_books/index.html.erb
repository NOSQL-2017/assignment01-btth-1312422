<p id="notice"><%= notice %></p>

<h1>Kind Of books</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @kind_of_books.each do |kind_of_book| %>
      <tr>
        <td><%= link_to 'Show', kind_of_book %></td>
        <td><%= link_to 'Edit', edit_kind_of_book_path(kind_of_book) %></td>
        <td><%= link_to 'Destroy', kind_of_book, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

